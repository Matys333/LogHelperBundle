<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="log.helper.message_handler" class="Matys333\LogHelperBundle\MessageHandler\LogMessageHandler">
            <!-- Will be filled in with dynamically -->
            <argument>%log_helper.backups.logs_path%</argument>
            <argument>%log_helper.backups.logs_backup_path%</argument>
            <argument>%log_helper.backups.self_log_file_name%</argument>
            <argument>%log_helper.backups.logs%</argument>
        </service>
    </services>
</container>